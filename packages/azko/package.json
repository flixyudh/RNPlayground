{
  "name": "azko",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --no-packager",
    "ios": "react-native run-ios --no-packager",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc",
    "bundle:ios": "react-native bundle --platform ios --entry-file index.js --dev false",
    "bundle:android": "react-native bundle --platform android --entry-file index.js --dev false",
    "pods": "(cd ios && bundle install && bundle exec pod install)",
    "pods:update": "(cd ios && bundle exec pod update)",
    "align-deps": "rnx-align-deps --write",
    "check-deps": "rnx-align-deps"
  },
  "dependencies": {
    "@module-federation/enhanced": "^0.21.4",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/native-stack": "6.11.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "^4.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@callstack/repack": "^5.2.2",
    "@react-native-community/cli": "^20.0.0",
    "@react-native/babel-preset": "^0.81.0",
    "@react-native/eslint-config": "0.81.5",
    "@react-native/metro-config": "^0.81.0",
    "@react-native/typescript-config": "0.81.5",
    "@rnx-kit/align-deps": "^3.3.2",
    "@rspack/core": "^1.3.4",
    "@swc/helpers": "^0.5.17",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "super-app-showcase-sdk": "0.0.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "presets": [
        "./node_modules/super-app-showcase-sdk/preset"
      ],
      "requirements": [
        "super-app-showcase-sdk@0.0.2"
      ],
      "capabilities": [
        "super-app"
      ]
    }
  }
}
